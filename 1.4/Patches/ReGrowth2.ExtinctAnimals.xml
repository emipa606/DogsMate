<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="RG_WoollyMammoth"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Elephant"]/pawnKinds</xpath>
      <value>
        <li>RG_WoollyMammoth</li>
      </value>
    </match>
  </Operation>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="RG_WoollyRhinoceros"]</xpath>
