<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="AEXP_Walrus"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Walrus"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Walrus</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Seal"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Seal</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>