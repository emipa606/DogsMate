<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="SnowRat"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Rat"]/pawnKinds</xpath>
          <value>
            <li>SnowRat</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
