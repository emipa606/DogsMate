<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="Sangrena"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Tiger"]/pawnKinds</xpath>
          <value>
            <li>Sangrena</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Dog"]/pawnKinds</xpath>
          <value>
            <li>Rimshepherd</li>
            <li>Mala</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Wolf"]/pawnKinds</xpath>
          <value>
            <li>Megawolf</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Bear"]/pawnKinds</xpath>
          <value>
            <li>Megabear</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
