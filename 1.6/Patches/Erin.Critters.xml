<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="Greyfox"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Fisher"]/pawnKinds</xpath>
          <value>
            <li>Fisher</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Fox"]/pawnKinds</xpath>
          <value>
            <li>Greyfox</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Hedgehog"]/pawnKinds</xpath>
          <value>
            <li>Hedgehog</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Armadillo"]/pawnKinds</xpath>
          <value>
            <li>Armadillo</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
