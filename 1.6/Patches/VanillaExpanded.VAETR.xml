<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingDef[defName="AEXP_Jaguar"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Jaguar"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Jaguar</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Tapir"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Tapir</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Tiger"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Tiger</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Gorilla"]/pawnKinds</xpath>
          <value>
            <li>AEXP_Gorilla</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
