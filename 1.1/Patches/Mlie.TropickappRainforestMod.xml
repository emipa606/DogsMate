<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Tropi[CKAPP] Rainforest Mod (Continued)</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Hare"]/pawnKinds</xpath>
          <value>
            <li>CK_ArcticHare</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Jaguar"]/pawnKinds</xpath>
          <value>
            <li>CK_Jaguar</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Leopard"]/pawnKinds</xpath>
          <value>
            <li>CK_SnowLeopard</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Panda"]/pawnKinds</xpath>
          <value>
            <li>CK_Panda</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Tapir"]/pawnKinds</xpath>
          <value>
            <li>CK_Tapir</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Tiger"]/pawnKinds</xpath>
          <value>
            <li>CK_Tiger</li>
            <li>CK_WhiteTiger</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
