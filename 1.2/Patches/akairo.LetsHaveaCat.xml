            <li>akaNEKO_Scottish_Fold_Long</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Let's Have a Cat!</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/Revolus.DogsMate.AnimalGroupDef[defName="Cat"]/pawnKinds</xpath>
          <value>
            <!-- Races_Animal_aka_NEKO_A.xml -->
            <li>akaNEKO_Siamese</li>
            <!-- Races_Animal_aka_NEKO_B.xml -->
            <li>akaNEKO_Persian</li>
            <li>akaNEKO_Maine_Coon</li>
            <li>akaNEKO_N_Forest_Cat</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
